{
  "name": "cursuri",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 33990",
    "build": "next build",
    "build:win": "powershell -Command \"$tempDir = Join-Path $PWD '.build-temp'; New-Item -ItemType Directory -Force -Path $tempDir | Out-Null; $env:TEMP = $tempDir; $env:TMP = $tempDir; npm run build\"",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "merge": "git checkout main && git merge dev && git push origin main && git checkout dev",
    "update:all": "npx npm-check-updates -u && npm i",
    "type-check": "tsc --noEmit > type-errors.log 2>&1",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@heroui/react": "^2.8.5",
    "@sentry/nextjs": "^10.21.0",
    "@tinymce/tinymce-react": "^6.3.0",
    "@types/uuid": "^11.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "date-fns": "^4.1.0",
    "ffmpeg-static": "^5.2.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "firewand": "^0.5.20",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.24",
    "microsoft-cognitiveservices-speech-sdk": "^1.46.0",
    "next": "16.0.0",
    "next-intl": "^4.4.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "react": "^19",
    "react-dom": "^19.2.0",
    "stripe": "^19.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@codai/axiom-engine": "^1.0.23",
    "@codai/axiom-mcp": "^1.0.24",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
